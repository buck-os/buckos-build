load("//defs:package.bzl", "package")

# This is a Go library package for terminal progress rendering.
# It provides Go modules, not a binary executable.
package(
    build_rule = "go",
    name = "go-echelon",
    version = "1.4.1",
    url = "https://github.com/cirruslabs/echelon/archive/refs/tags/v1.4.1.tar.gz",
    sha256 = "afcba9e206c87ed7226b780612eb9cb55a465910684010e880d9cbf281e32c69",
    # Library package - no bins means default src_install will install Go source files
)
